{
	"compilerOptions": {
		"target": "esnext",
		"module": "commonjs",
		"moduleResolution": "node",
		"baseUrl": "./src",
		"jsx": "react",
		"noEmit": true,
		"allowSyntheticDefaultImports": true,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"paths": {
			"@models/*": ["models/*"],
			"@src/*": ["*"],
			"@client/*": ["client/*"]
		},
		"lib": ["es5", "es6", "dom", "esnext.asynciterable"]
	},
	"include": ["src"]
}
